{
    "name": "bvdputte/kirby-log",
    "description": "Logging utility for Kirby 3 - based on katzgrau/KLogger",
    "license": "MIT",
    "type": "kirby-plugin",
    "version": "2.2.0",
    "authors": [
      {
          "name": "Bart Vandeputte",
          "email": "mail@bartvandeputte.be"
      }
    ],
    "require": {
        "getkirby/composer-installer": "^1.1",
        "katzgrau/klogger": "^1.2",
        "studio24/rotate": "^1.0"
    },
    "autoload": {
      "psr-4": {
          "bvdputte\\kirbyLog\\": "src/"
      }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "allow-plugins": {
            "getkirby/composer-installer": true
        }
    }
}
